<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>netstandard2.0;netstandard2.1</TargetFrameworks>
		<RootNamespace>Xmf2.Common</RootNamespace>
		<LangVersion>latest</LangVersion>
	</PropertyGroup>

	<PropertyGroup>
		<PackageVersion>1.0.1</PackageVersion>
		<Title>Xmf2.Common.Extensions</Title>
		<PackageId>Xmf2.Common.Extensions</PackageId>
		<Authors>Ideine, VJubert</Authors>
		<Description>Ideine extensions for netstandard class</Description>
		<Copyright>2020 Ideine</Copyright>
		<PackageTags>xmf2 ideine extensions netstandard</PackageTags>
		<PackageReleaseNotes>Initial release</PackageReleaseNotes>
	</PropertyGroup>

	<PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
	  <DebugSymbols>true</DebugSymbols>
	</PropertyGroup>

	<ItemGroup>
	  <None Remove=".editorconfig" />
	  <None Remove=".gitignore" />
	  <None Remove="misc/.azure.ci.yml" />
	</ItemGroup>

</Project>
